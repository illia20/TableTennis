@model IEnumerable<TableTennis.Models.User>
<h2>List of users</h2>
<table class="table">
    @foreach(var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Mods of access</a>
            </td>
        </tr>
    }
</table>

